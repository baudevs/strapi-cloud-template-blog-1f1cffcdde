{
  "kind": "singleType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "Contact Page",
    "description": "Contact page content with dynamic sections"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "hero": {
      "type": "component",
      "component": "shared.hero-section",
      "repeatable": false
    },
    "contactDetails": {
      "type": "component",
      "component": "shared.contact-details",
      "repeatable": false
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "sections.features",
        "sections.faq",
        "sections.testimonials",
        "sections.cta",
        "sections.hubspot-form",
        "shared.rich-text",
        "shared.media",
        "shared.quote"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "footer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::footer.footer"
    }
  }
}
